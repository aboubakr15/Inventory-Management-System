<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="375.0" stylesheets="@../../style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lightsaberInventory.View.ModifyClientController">
    <children>
          <Label fx:id="clientName" alignment="CENTER" layoutY="36.0" prefHeight="16.0" prefWidth="375.0" text="Modify Part">
              <font>
                  <Font name="Trebuchet MS Bold" size="20.0" />
              </font>
          </Label>
          <Button fx:id="saveButton" layoutX="90.0" layoutY="338.0" mnemonicParsing="false" onAction="#saveMod" prefHeight="30.0" prefWidth="80.0" text="Save" />
          <Button fx:id="cancelButton" layoutX="212.0" layoutY="338.0" mnemonicParsing="false" onAction="#setCancelButton" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: red;" text="Cancel" />
          <Label layoutX="80.0" layoutY="143.0" text="Name">
         <font>
            <Font size="18.0" />
         </font>
          </Label>
          <TextField fx:id="textName" alignment="CENTER_RIGHT" layoutX="164.0" layoutY="142.0" />
      <Label layoutX="77.0" layoutY="199.0" text="Phone">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="textPhoneNumber" alignment="CENTER_RIGHT" layoutX="164.0" layoutY="198.0" />
      <Label layoutX="77.0" layoutY="251.0" text="Address">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="textAddress" alignment="CENTER_RIGHT" layoutX="164.0" layoutY="250.0" />
    </children>
</AnchorPane>
