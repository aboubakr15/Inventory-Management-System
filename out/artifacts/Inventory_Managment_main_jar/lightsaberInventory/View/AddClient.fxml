               </HBox.margin>
            </Button>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#setCancelButton" prefHeight="30.0" prefWidth="80.0" text="Cancel">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" layoutX="72.0" layoutY="301.0" prefHeight="26.0" prefWidth="219.0" />
      <Label layoutX="132.0" layoutY="84.0" text="Add Client">
         <font>
            <Font name="Trebuchet MS Bold" size="20.0" />
         </font>
      </Label>
