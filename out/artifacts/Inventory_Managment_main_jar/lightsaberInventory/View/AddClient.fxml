<AnchorPane fx:id="addClientPain" prefHeight="500.0" prefWidth="375.0" stylesheets="@../../style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lightsaberInventory.View.AddClientController">
   <children>
