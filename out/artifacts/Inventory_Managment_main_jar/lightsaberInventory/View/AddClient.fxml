               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textAddress" prefHeight="25.0" prefWidth="176.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="327.0" prefHeight="38.0" prefWidth="375.0">
         <children>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addClientButtonPressed" prefHeight="30.0" prefWidth="80.0" text="Add">
