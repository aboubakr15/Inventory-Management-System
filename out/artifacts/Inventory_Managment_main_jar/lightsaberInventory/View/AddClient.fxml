         <children>
            <Label prefHeight="22.0" prefWidth="123.0" text="Name">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textName" alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="174.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="21.0" layoutY="203.0" prefHeight="25.0" prefWidth="300.0">
         <children>
            <Label prefHeight="22.0" prefWidth="133.0" text="Phone">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textPhoneNumber" alignment="CENTER_RIGHT" prefHeight="26.0" prefWidth="178.0" />
